# .env.prod продакшен в Google Cloud Run
# ВНИМАНИЕ: Этот файл используется только для справки
# В продакшн все секреты берутся из Google Cloud Secret Manager

GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=europe-west1

# Настройки приложения
ENVIRONMENT=production
DEBUG_LOGGING=false
API_PORT=8080

# Секреты (в продакшн берутся из Secret Manager)
# OPENAI_API_KEY - из секрета openai-api-key
# JWT_SECRET_KEY - из секрета jwt-secret-key
# CRYPTOCOMPARE_API_KEY - из секрета cryptocompare-api-key
# TELEGRAM_BOT_TOKEN - из секрета telegram-bot-token

# Настройки по умолчанию
DEFAULT_SYMBOL=BTCUSDT
LLM_PROVIDER=openai
ADMIN_TELEGRAM_ID=299820674
